<?xml version="1.0" encoding="UTF-8"?>
<openbox_menu xmlns="http://openbox.org/3.4/menu">
	<menu id="apps-accessories-menu" label="Accessories">
		<item label="Calculator">
			<action name="Execute">
				<command>gnome-calculator</command>
				<startupnotify>
					<enabled>yes</enabled>
				</startupnotify>
			</action>
		</item>
		<item label="JDoTxt">
			<action name="Execute">
				<command>java -jar ~/.jdotxt/jdotxt.jar</command>
			</action>
		</item>
		<item label="Gcolor2">
			<action name="Execute">
				<command>gcolor2</command>
			</action>
		</item>
		<item label="Nautilus">
			<action name="Execute">
				<command>nautilus --no-desktop --browser</command>
				<startupnotify>
					<enabled>yes</enabled>
				</startupnotify>
			</action>
		</item>
		<item label="Xfce Terminal">
			<action name="Execute">
				<command>xfce4-terminal</command>
				<startupnotify>
					<enabled>yes</enabled>
				</startupnotify>
			</action>
		</item>
	</menu>
	<menu id="apps-dev-menu" label="Development">
		<item label="Eclipse">
			<action name="Execute">
				<command>~/.eclipse.sh</command>
			</action>
		</item>
		<item label="Geany">
			<action name="Execute">
				<command>geany</command>
			</action>
		</item>
		<item label="Meld">
			<action name="Execute">
				<command>~/.meld.sh</command>
			</action>
		</item>
		<item label="Nemiver">
			<action name="Execute">
				<command>nemiver</command>
			</action>
		</item>
		<item label="QtCreator">
			<action name="Execute">
				<command>qtcreator</command>
			</action>
		</item>
	</menu>
	<menu id="apps-net-menu" label="Internet">
		<item label="Chromium">
			<action name="Execute">
				<command>chromium</command>
				<startupnotify>
					<enabled>yes</enabled>
					<wmclass>Chromium</wmclass>
				</startupnotify>
			</action>
		</item>
		<item label="Firefox">
			<action name="Execute">
				<command>~/.firefox.sh</command>
				<startupnotify>
					<enabled>yes</enabled>
					<wmclass>Firefox</wmclass>
				</startupnotify>
			</action>
		</item>
		<item label="Steam">
			<action name="Execute">
				<command>steam</command>
			</action>
		</item>
		<item label="Remote Transmission">
			<action name="Execute">
				<command>transmission-remote-gtk</command>
			</action>
		</item>
		<item label="Unison">
			<action name="Execute">
				<command>unison-gtk2</command>
			</action>
		</item>
		<item label="Wireshark">
			<action name="Execute">
				<command>wireshark</command>
			</action>
		</item>
	</menu>
	<menu id="apps-office-menu" label="Office">
		<item label="LibreOffice">
			<action name="Execute">
				<command>libreoffice</command>
			</action>
		</item>
		<item label="LibreOffice Calc">
			<action name="Execute">
				<command>libreoffice --calc</command>
			</action>
		</item>
		<item label="LibreOffice Draw">
			<action name="Execute">
				<command>libreoffice --draw</command>
			</action>
		</item>
		<item label="LibreOffice Impress">
			<action name="Execute">
				<command>libreoffice --impress</command>
			</action>
		</item>
		<item label="LibreOffice Writer">
			<action name="Execute">
				<command>ooffice -writer</command>
			</action>
		</item>
	</menu>
	<menu id="apps-multimedia-menu" label="Multimedia">
		<item label="Inkscape">
			<action name="Execute">
				<command>inkscape</command>
			</action>
		</item>
		<item label="Gimp">
			<action name="Execute">
				<command>gimp</command>
				<startupnotify>
					<enabled>yes</enabled>
				</startupnotify>
			</action>
		</item>
		<item label="Shotwell">
			<action name="Execute">
				<command>shotwell</command>
			</action>
		</item>
		<separator />
		<item label="Baka MPlayer">
			<action name="Execute">
				<command>baka-mplayer</command>
			</action>
		</item>
		<item label="EasyTag">
			<action name="Execute">
				<command>easytag</command>
			</action>
		</item>
		<item label="Gnome Subtitles">
			<action name="Execute">
				<command>gnome-subtitles</command>
			</action>
		</item>
		<item label="Pitivi">
			<action name="Execute">
				<command>pitivi</command>
			</action>
		</item>
		<item label="Rhythmbox">
			<action name="Execute">
				<command>rhythmbox</command>
				<startupnotify>
					<enabled>yes</enabled>
				</startupnotify>
			</action>
		</item>
		<item label="SoundConverter">
			<action name="Execute">
				<command>soundconverter</command>
			</action>
		</item>
	</menu>
	<menu id="apps-graphics-menu" label="Graphics">
		<item label="Gwenview">
			<action name="Execute">
				<command>gwenview</command>
				<startupnotify>
					<enabled>yes</enabled>
				</startupnotify>
			</action>
		</item>
		<item label="Dia Diagram Editor">
			<action name="Execute">
				<command>dia</command>
				<startupnotify>
					<enabled>yes</enabled>
				</startupnotify>
			</action>
		</item>
		<item label="Inkscape">
			<action name="Execute">
				<command>inkscape</command>
				<startupnotify>
					<enabled>yes</enabled>
				</startupnotify>
			</action>
		</item>
	</menu>
	<menu id="system-menu" label="System">
		<item label="ARandr">
			<action name="Execute">
				<command>arandr</command>
			</action>
		</item>
		<item label="Gnome System Monitor">
			<action name="Execute">
				<command>gnome-system-monitor</command>
			</action>
		</item>
		<item label="Manage Cups Printers">
			<action name="Execute">
				<command>xdg-open http://localhost:631/</command>
				<startupnotify>
					<enabled>no</enabled>
					<icon>cups</icon>
				</startupnotify>
			</action>
		</item>
		<separator />
		<item label="Openbox Configuration Manager">
			<action name="Execute">
				<command>obconf</command>
				<startupnotify>
					<enabled>yes</enabled>
				</startupnotify>
			</action>
		</item>
		<item label="LxAppearance">
			<action name="Execute">
				<command>lxappearance</command>
				<startupnotify>
					<enabled>yes</enabled>
				</startupnotify>
			</action>
		</item>
		<item label="Tint Configuration">
			<action name="Execute">
				<command>tintwizard</command>
			</action>
		</item>
		<item label="Change Background">
			<action name="Execute">
				<command>nitrogen</command>
			</action>
		</item>
		<separator />
		<item label="Reconfigure Openbox">
			<action name="Reconfigure" />
		</item>
	</menu>
	<menu id="close-menu" label="Close">
		<item label="Close OpenBox">
			<action name="Exit">
				<prompt>yes</prompt>
			</action>
		</item>
		<item label="Lock">
			<action name="Execute">
				<command>i3lock -d -c 000000</command>
			</action>
		</item>
		<item label="Reboot">
			<action name="Execute">
				<command>reboot</command>
				<prompt>yes</prompt>
			</action>
		</item>
		<item label="Shutdown">
			<action name="Execute">
				<command>halt</command>
				<prompt>yes</prompt>
			</action>
		</item>
	</menu>
	<menu id="root-menu" label="Openbox 3">
		<separator label="Applications" />
		<menu id="apps-accessories-menu" />
		<menu id="apps-dev-menu" />
		<menu id="apps-net-menu" />
		<menu id="apps-office-menu" />
		<menu id="apps-multimedia-menu" />
		<separator label="System" />
		<menu id="system-menu" />
		<menu id="close-menu" />
	</menu>
</openbox_menu>
